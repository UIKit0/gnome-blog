#: hig_alert.py:13
#, python-format
msgid "URL '%s' may not be a valid bloggerAPI. XML-RPC Server reported: %s."
msgstr "URL '%s' možná není platné bloggerAPI. Server XML-RPC odpověděl: %s."

#: hig_alert.py:15
#, python-format
msgid ""
"Unknown username %s or password trying to post blog entry to XML-RPC server %"
"s."
msgstr ""
"Neznámé jméno uživatele %s nebo heslo při pokusu vystavit položku blogu na "
"XML-RPC server %s."

#: hig_alert.py:17
#, python-format
msgid ""
"Could not post to blog '%s' at bloggerAPI XML-RPC server '%s'. Server "
"reported: %s."
msgstr ""
"Nemohu vystavit na blog '%s' na XML-RPC serveru bloggerAPI '%s'. Server "
"odpověděl: %s."

#: hig_alert.py:19
#, python-format
msgid ""
"The bloggerAPI server (%s) reported an error I don't understand: '%s, %s'. \n"
"\n"
"Please email this error message to seth@gnome.org so I can address it."
msgstr ""
"Server bloggerAPI (%s) ohlásil chybu, které nerozumím: '%s, %s.\n"
"\n"
"Pošlete prosím o tomto hlášení chyby e-mail na seth@gnome.org, abych na to "
"mohl reagovat."

